<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <link rel="icon" href="/images/favicon.svg">
    <link rel="stylesheet" href="/css/site.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>DevCycle .NET Example</title>
  </head>

  <body>
    <div class="App">
      <div class="App-header">
        <p>Demo Application</p>
        <img
          height="46"
          src="/images/devcycle-togglebot-full-colour.svg"
          alt="DevCycle"
        />
      </div>
      
      <div class="App-wrapper">
        @RenderBody()
      </div>

      <p>Reload the page to view changes.</p>
  
      <a
        class="App-link"
        href="https://docs.devcycle.com/sdk/server-side-sdks/dotnet/"
        target="_blank"
        rel="noopener noreferrer"
      >
        DevCycle .NET SDK Docs
      </a>
    </div>

    @await RenderSectionAsync("Scripts", required: false)
  </body>
</html>
